{
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "PIP_TARGET": ".",
      "PIP_UPGRADE": "true"
    }
  }
} 